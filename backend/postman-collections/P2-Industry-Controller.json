							"script": {
								"exec": [
									"// 設定測試資料",
									"pm.environment.set('test_new_industry_name', '測試產業_' + Date.now());",
									"pm.environment.set('test_new_industry_title', '測試產業標題');",
									"pm.environment.set('test_new_industry_description', '這是一個測試產業的描述');",
									"pm.environment.set('test_new_industry_icon', '/icons/test.png');"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Industry created successfully', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('id');",
									"    pm.expect(jsonData).to.have.property('name');",
									"    pm.expect(jsonData).to.have.property('title');",
									"    pm.expect(jsonData.name).to.eql(pm.environment.get('test_new_industry_name'));",
									"    ",
									"    // 儲存新創建的產業ID供後續測試使用",
									"    pm.environment.set('test_created_industry_id', jsonData.id);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
